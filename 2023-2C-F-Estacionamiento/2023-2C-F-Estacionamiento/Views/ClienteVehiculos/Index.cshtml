@model IEnumerable<_2023_2C_F_Estacionamiento.Models.ClienteVehiculo>

@{
    ViewData["Title"] = "Vehiculos Autorizados";
}


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vehiculo.Patente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResponsablePrincipal)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.NombreCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vehiculo.Patente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResponsablePrincipal)
            </td>
            <td>
                <button type="button" class="boton-3d" onclick="location.href='@Url.Action("Details", "ClienteVehiculos", new { id = item.VehiculoId })'">Detalles</button>

              @*   @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ }) *@
            </td>
        </tr>
}
    </tbody>
</table>
